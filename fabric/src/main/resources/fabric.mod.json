{
  "schemaVersion": 1,
  "id": "pl3xmap",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "BillyGalbreath",
    "JLyne"
  ],
  "contact": {
    "homepage": "${website}",
    "sources": "${sources}",
    "issues": "${issues}"
  },
  "license": "MIT",
  "icon": "pl3xmap.png",
  "environment": "server",
  "entrypoints": {
    "server": [
      "${group}.Pl3xMapFabric"
    ]
  },
  "mixins": [
    "pl3xmap.mixins.json"
  ],
  "accessWidener": "pl3xmap.accesswidener",
  "depends": {
    "fabricloader": ">=${fabricLoaderVersion}",
    "fabric": ">=${fabricApiVersion}",
    "minecraft": "~${minecraftVersion}",
    "java": ">=17"
  },
  "recommends": {
    "better-fabric-console": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/nhGzEkwXQX"
      }
    }
  }
}
